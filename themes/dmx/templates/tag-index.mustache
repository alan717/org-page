<div class="container center-block">
	<div>
		<div class="post" id="tags" hidden>
			<h1>Tag Index</h1>
			<ul class="org-ul">
				{{#tags}}
					<li><a tag-weight="{{count}}" href="{{tag-uri}}">{{tag-name}}</a></li>
				{{/tags}}
			</ul>
		</div>
	</div>

	<div id="myCanvasContainer">
		<canvas width="800" height="494" id="myCanvas">
			<p>Anything in here will be replaced on browsers that support the canvas element</p>
		</canvas>
	</div>
</div>

